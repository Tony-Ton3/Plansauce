    "watch": [
        "*.js",
        "routes/",
        "controllers/",
        "models/",
        "utils/"
    ],
    "ignore": [
        "node_modules/"
    ],
    "ext": "js,json",
    "delay": "500",
    "verbose": true,
    "env": {
        "NODE_ENV": "development"
    }
}